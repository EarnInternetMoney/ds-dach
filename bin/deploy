#!/usr/bin/env bash
set -ex

export ETH_GAS=${ETH_GAS:-"7000000"}

#Kovan deployment
export ETH_RPC_URL=https://kovan.infura.io
export DAI=0xaaeb56503ceb8852f802bdf050b8ff7d567716ed
export UNISWAP_EXCHANGE=0x0dbe60db8145d829b339e31e979fcc4b0c1179ca
seth send --create out/Dach.bin "Dach(address,address,string memory,uint256)" $DAI $UNISWAP_EXCHANGE "1" $(seth --to-uint256 42)
